<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WaffleButton">
        <attr name="waffleButtonStyle" format="enum">
            <enum name="Default" value="0"/>
            <enum name="Primary" value="1"/>
            <enum name="Done" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="WaffleCountView">
        <attr name="colorEmpty" format="color"/>
        <attr name="colorNormal" format="color"/>
        <attr name="colorAlert" format="reference"/>
        <attr name="maxCount" format="integer"/>
        <attr name="count" format="integer"/>
    </declare-styleable>
</resources>